{
  "name": "react-ssr-boilerplate",
  "version": "1.0.0",
  "description": "React server side rendering boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel webpack:*",
    "webpack:start": "nodemon --watch build --exec node build/bundle.js",
    "webpack:server": "webpack --config webpack.server.js --mode development --watch",
    "webpack:client": "webpack --config webpack.client.js --mode development --watch"
  },
  "keywords": [
    "react",
    "react-ssr",
    "server-side-rendering"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ganeshmunisifreddy/react-ssr-boilerplate.git"
  },
  "author": "Ganesh Munisifreddy",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.3.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^6.7.3",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^2.7.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.5"
  }
}
